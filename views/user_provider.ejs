<div class="modal fade show-facility" id="editModal" tabindex="-1" aria-labelledby="editModalLabel" aria-hidden="true">
  <div class="modal-dialog m-0-sm">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="editModalLabel" >Facility Name - Edit</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true"><i class="fas fa-times"></i></span>
        </button>
      </div>
      <div class="modal-body" style="padding-right:0;padding-left:0">
        <div class="">
          <form class="formFacitlity" action="index.html" method="post">
            <div class="alert alert-primary" style="border-radius: 0;width:100%" role="alert">
              Basic Information
            </div>
            <div class="d-sm-flex flex-wrap px-4 justify-content-md-start justify-content-between">
              <div class="form-group col-sm-4">
                <label for="formGroupInput">Network Name</label>
                <input type="text" class="form-control" id="formGroupInput" placeholder="Example input placeholder">
              </div>
              <div class="form-group col-sm-4">
                <label for="formGroupInput2">Mail Address</label>
                <input type="text" class="form-control" id="formGroupInput2" placeholder="Another input placeholder">
              </div>
              <div class="form-group col-sm-4">
                <label for="formGroupInput2">Phone number</label>
                <input type="text" class="form-control" id="formGroupInput2" placeholder="+14(0)145 547 12">
              </div>
              <div class="form-group col-sm-4">
                  <label for="validationTooltip04">City</label>
                  <select class="custom-select" id="cityselect">
                    <option selected disabled value="">New-York</option>
                    <option>...</option>
                  </select>
              </div>
              <div class="form-group col-sm-4">
                  <label for="validationTooltip04">State</label>
                  <select class="custom-select" id="stateselect">
                    <option selected disabled value="">USA</option>
                    <option>...</option>
                  </select>
              </div>
              <div class="form-group col-sm-4">
                <label for="formGroupInput2">Website</label>
                <input type="text" class="form-control" id="formGroupInput2" placeholder="www.hicard.com">
              </div>
            </div>
            <div class="alert alert-primary" style="border-radius: 0;width:100%" role="alert">
              Address Information
            </div>
            <div class="d-sm-flex flex-wrap justify-content-md-start justify-content-between px-4">
              <div class="form-group col-sm-4">
                <label for="formGroupInput">address 1</label>
                <input type="text" class="form-control" id="formGroupInput" placeholder="Example input placeholder">
              </div>
              <div class="form-group col-sm-4">
                <label for="formGroupInput2">Address 2</label>
                <input type="text" class="form-control" id="formGroupInput2" placeholder="Another input placeholder">
              </div>
              <div class="form-group col-sm-4">
                <label for="formGroupInput2">Postal Code</label>
                <input type="text" class="form-control" id="formGroupInput2" placeholder="Another input placeholder">
              </div>
              <div class="form-group col-sm-4">
                <label for="formGroupInput">Merely To</label>
                <input type="text" class="form-control" id="formGroupInput" placeholder="Example input placeholder">
              </div>
              <div class="form-group col-sm-4">
                <label for="formGroupInput2">Objective</label>
                <input type="text" class="form-control" id="formGroupInput2" placeholder="Another input placeholder">
              </div>

            </div>
            <div class="form-group col-sm-12 px-4">
              <label for="validationTextarea">Textarea</label>
              <textarea class="form-control " id="validationTextarea" placeholder=" example textarea"></textarea>

            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
              <button type="submit" class="btn btn-primary">Save change</button>
            </div>
          </form>
        </div>

      </div>

    </div>
  </div>
</div>
